{
  "name": "hosted-overlay",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "firebase-login": "firebase login",
    "firebase-init": "firebase init",
    "emulate": "firebase emulators:start --inspect-functions",
    "emulate:import": "firebase emulators:start --import=db --inspect-functions",
    "e:i": "npm run emulate:import",
    "export": "firebase emulators:export db",
    "deploy": "firebase deploy",
    "deploy:only": "firebase deploy --only",
    "deploy:functions": "firebase deploy --only functions",
    "deploy:rules": "firebase deploy --only database,storage,firestore",
    "deploy:hosting": "firebase deploy --only hosting",
    "indexes:fetch": "firebase firestore:indexes > firestore.indexes.json",
    "config:get": "firebase functions:config:get",
    "config:set": "firebase functions:config:set",
    "config:unset": "firebase functions:config:unset",
    "config:fetch": "firebase functions:config:get > .runtimeconfig.json"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "firebase": "^8.7.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "twitch-js": "^2.0.0-beta.42",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "eslint-config-prettier": "^8.3.0",
    "firebase-tools": "^9.16.0",
    "prettier": "^2.3.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
