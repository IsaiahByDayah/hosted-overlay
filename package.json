{
  "name": "hosted-overlay",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "firebase:login": "firebase login",
    "firebase:init": "firebase init",
    "dev": "yarn start",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint './src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint './src/**/*.{js,jsx,ts,tsx}' --fix",
    "format": "prettier --write './src/**/*.{js,jsx,ts,tsx}'",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "shell": "firebase functions:shell",
    "emulate": "firebase emulators:start --inspect-functions",
    "emulate:import": "firebase emulators:start --import=db --inspect-functions",
    "e:i": "npm run emulate:import",
    "export": "firebase emulators:export db",
    "deploy": "firebase deploy",
    "deploy:only": "firebase deploy --only",
    "deploy:functions": "firebase deploy --only functions",
    "deploy:rules": "firebase deploy --only database,storage,firestore",
    "deploy:hosting": "firebase deploy --only hosting",
    "indexes:fetch": "firebase firestore:indexes > firestore.indexes.json",
    "config:get": "firebase functions:config:get",
    "config:set": "firebase functions:config:set",
    "config:unset": "firebase functions:config:unset",
    "config:fetch": "firebase functions:config:get > .runtimeconfig.json"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.1",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "clsx": "^1.1.1",
    "faker": "^5.5.3",
    "firebase": "^8.7.1",
    "number-abbreviate": "^2.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "twitch-js": "^2.0.0-beta.42",
    "typeface-nunito": "^1.1.13",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.4",
    "@storybook/addon-essentials": "^6.3.4",
    "@storybook/addon-links": "^6.3.4",
    "@storybook/addon-storyshots": "^6.3.4",
    "@storybook/node-logger": "^6.3.4",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.4",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/faker": "^5.5.7",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.8",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "firebase-tools": "^9.16.0",
    "prettier": "^2.3.2",
    "typescript": "^4.1.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier"
    ],
    "rules": {
      "semi": [
        0,
        "never"
      ]
    },
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "babel-loader": "8.1.0"
  }
}
